{
  "name": "Simple Time Tracker",
  "version": "1.0.0",
  "description": "A very basic extension to track time spent on the current tab.",
  "permissions": ["activeTab"],
  "icons": {
    "16": "assets/icon-clock-16x16.png",
    "48": "assets/icon-clock-48x48.png",
    "128": "assets/icon-clock-128x128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "assets/icon-clock-48x48.png"
  },
  "manifest_version": 2
}
